{
  "name": "react-portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "React Portfolio of bruggineer (Teresa Bruggeman)",
  "main": "server.js",
  "proxy": "http://localhost/8080/",
  "homepage": "https://bruggineer.github.io/PortfolioUpdated/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bruggineer/PortfolioUpdated.git"
  },
  "keywords": [
    "react",
    "fullstack",
    "MERN",
    "developer"
  ],
  "author": "Teresa Bruggeman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bruggineer/PortfolioUpdated/issues"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "nodemon server.js & react-scripts start",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "rm -r build && npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "debug": "^4.3.1",
    "emailjs-com": "^2.6.4",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "immer": "^8.0.1",
    "jquery": "^3.5.1",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^3.11.0",
    "react-modals": "0.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "react-sliding-pane": "^6.1.2",
    "usecaptcha": "^1.0.4"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
